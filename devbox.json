{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.16.0/.schema/devbox.schema.json",
  "packages": [
    "github:dagger/nix/6215bc1f0c3e6337557cd2660bc81835431fc6f2#packages.x86_64-linux.dagger",
    "go@1.25.0",
    "gh@2.79.0",
    "golangci-lint@2.4.0",
    "go-tools@2025.1.1",
    "errcheck@1.9.0",
    "delve@1.25.1",
    "pprof@0-unstable-2025-03-02",
    "bat@0.25.0",
    "jq@1.8.1",
    "lefthook@1.12.4"
  ],
  "shell": {
    "init_hook": [
      "alias cat='bat --theme 1337'",
      "eval \"$(dagger completion bash)\"",
      "eval \"$(lefthook completion bash)\"",
      "lefthook install",
      "lefthook run pre-commit --all-files",
    ]
  }
}
